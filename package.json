{
  "name": "dolphinjs",
  "version": "1.0.0",
  "description": "A universal JSX framework for web, mobile, desktop, and embedded platforms with LVGL support.",
  "main": "index.js",
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./core/*": "./core/*.js",
    "./systems/*": "./systems/*.js",
    "./utils/*": "./utils/*.js",
    "./build/*": "./build/*.js",
    "./compiler/*": "./compiler/*.js",
    "./runtime/*": "./runtime/*.js",
    "./embedded/*": "./embedded/*.js",
    "./lvgl/*": "./lvgl/*.js",
    "./jsx-runtime": "./jsx-runtime.js",
    "./jsx-dev-runtime": "./jsx-dev-runtime.js"
  },
  "jest": {
  "transform": {
    "^.+\\.(js|jsx|mjs)$": "babel-jest"
  },
  "transformIgnorePatterns": [
    "node_modules/(?!(dolphinjs)/)"
  ],
  "moduleFileExtensions": ["js", "jsx", "mjs", "json", "node"],
  "extensionsToTreatAsEsm": [".js", ".jsx", ".mjs"],
  "testEnvironment": "node"
},
  "scripts": {
    "dev": "node examples/web-app.js",
    "dev:web": "node examples/web-app.js",
    "dev:node": "node examples/node-app.js",
    "dev:mobile": "node examples/mobile-app.js",
    "dev:embedded": "node examples/embedded-app.js",
    "dev:lvgl": "node examples/lvgl-app.js",
    "dev:arduino": "node examples/arduino-app.js",
    "dev:esp32": "node examples/esp32-app.js",
    "build": "npm run build:web && npm run build:node && npm run build:embedded",
    "build:web": "node build/build-system.js --target web --output ./dist/web",
    "build:node": "node build/build-system.js --target node --output ./dist/node",
    "build:android": "node build/build-system.js --target android --output ./dist/android",
    "build:ios": "node build/build-system.js --target ios --output ./dist/ios",
    "build:electron": "node build/build-system.js --target electron --output ./dist/electron",
    "build:embedded": "node build/build-system.js --target embedded --output ./dist/embedded",
    "build:lvgl": "node build/build-system.js --target lvgl --output ./dist/lvgl",
    "build:arduino": "node build/build-system.js --target arduino --output ./dist/arduino",
    "build:esp32": "node build/build-system.js --target esp32 --output ./dist/esp32",
    "build:all": "npm run build:web && npm run build:node && npm run build:embedded && npm run build:lvgl && npm run build:arduino && npm run build:esp32",
    "compile": "node compiler/cli.js",
    "compile:jsx": "node compiler/cli.js --jsx",
    "compile:c": "node compiler/cli.js --target c",
    "compile:cpp": "node compiler/cli.js --target cpp",
    "compile:lvgl": "node compiler/cli.js --target lvgl",
    "compile:arduino": "node compiler/cli.js --target arduino",
    "compile:esp32": "node compiler/cli.js --target esp32",
    "compile:all": "npm run compile:jsx && npm run compile:c && npm run compile:cpp && npm run compile:types",
    "compile:types": "tsc --project tsconfig.json --declaration --emitDeclarationOnly",
    "test": "npx cross-env NODE_OPTIONS='--experimental-vm-modules' jest --config=jest.config.js",
    "test:web": "npx cross-env NODE_OPTIONS='--experimental-vm-modules' jest --platform web --config=jest.config.js",
    "test:node": "npx cross-env NODE_OPTIONS='--experimental-vm-modules' jest --platform node --config=jest.config.js",
    "test:android": "npx cross-env NODE_OPTIONS='--experimental-vm-modules' jest --platform android --config=jest.config.js",
    "test:ios": "npx cross-env NODE_OPTIONS='--experimental-vm-modules' jest --platform ios --config=jest.config.js",
    "test:embedded": "npx cross-env NODE_OPTIONS='--experimental-vm-modules' jest --platform embedded --config=jest.config.js",
    "test:lvgl": "npx cross-env NODE_OPTIONS='--experimental-vm-modules' jest --platform lvgl --config=jest.config.js",
    "test:arduino": "npx cross-env NODE_OPTIONS='--experimental-vm-modules' jest --platform arduino --config=jest.config.js",
    "test:esp32": "npx cross-env NODE_OPTIONS='--experimental-vm-modules' jest --platform esp32 --config=jest.config.js",
    "test:coverage": "npx cross-env NODE_OPTIONS='--experimental-vm-modules' jest --coverage --config=jest.config.js",
    "test:watch": "npx cross-env NODE_OPTIONS='--experimental-vm-modules' jest --watch --config=jest.config.js",
    "test:sim": "node test/simulator/embedded-simulator.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,c,h,cpp}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,c,h,cpp}\"",
    "docs": "jsdoc -c jsdoc.config.json",
    "docs:serve": "serve docs/",
    "clean": "rm -rf dist/ build/ coverage/ .cache/",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "prepack": "npm run build && npm run test",
    "prepublishOnly": "npm run build && npm run test",
    "postinstall": "echo 'ðŸ¬ DolphinJS installed successfully!'",
    "start": "npm run dev:web",
    "serve": "serve dist/web -p 3000",
    "deploy": "npm run build:web && gh-pages -d dist/web",
    "benchmark": "node benchmark/benchmark.js",
    "benchmark:web": "node benchmark/web-benchmark.js",
    "benchmark:node": "node benchmark/node-benchmark.js",
    "benchmark:embedded": "node benchmark/embedded-benchmark.js",
    "dolphin": "node cli/dolphin-cli.js",
    "create-app": "node cli/create-app.js",
    "create:web": "node cli/create-app.js --template web",
    "create:mobile": "node cli/create-app.js --template mobile",
    "create:desktop": "node cli/create-app.js --template electron",
    "create:embedded": "node cli/create-app.js --template embedded",
    "create:lvgl": "node cli/create-app.js --template lvgl",
    "create:arduino": "node cli/create-app.js --template arduino",
    "create:esp32": "node cli/create-app.js --template esp32",
    "flash:arduino": "platformio run --target upload",
    "flash:esp32": "platformio run --target upload --environment esp32dev",
    "monitor": "platformio device monitor",
    "sim:embedded": "node embedded/simulator/embedded-simulator.js",
    "sim:lvgl": "node lvgl/simulator/lvgl-simulator.js",
    "sim:web": "serve embedded/simulator/web -p 8080",
    "setup:embedded": "npm run setup:platformio && npm run setup:cmake",
    "setup:platformio": "pip install platformio || echo 'Install Python and pip first'",
    "setup:cmake": "brew install cmake || sudo apt-get install cmake || choco install cmake",
    "setup:all": "npm install && npm run setup:embedded",
    "generate:lvgl-bindings": "node tools/generate-lvgl-bindings.js",
    "generate:embedded-bindings": "node tools/generate-embedded-bindings.js",
    "generate:bindings": "npm run generate:lvgl-bindings && npm run generate:embedded-bindings",
    "docker:embedded": "docker build -t dolphinjs-embedded -f docker/embedded.Dockerfile .",
    "docker:lvgl": "docker build -t dolphinjs-lvgl -f docker/lvgl.Dockerfile .",
    "docker:run": "docker run -it dolphinjs-embedded",
    "example:blinky": "node examples/embedded/blinky.js --sim",
    "example:sensors": "node examples/embedded/sensors.js --sim",
    "example:dashboard": "node examples/embedded/dashboard.js --sim",
    "example:lvgl-test": "node examples/lvgl/lvgl-test.js --sim",
    "example:ui-demo": "node examples/lvgl/ui-demo.js --sim",
    "mobile:serve": "cd mobile-pwa && python -m http.server 3000",
    "mobile:open": "open mobile-app.html || start mobile-app.html || xdg-open mobile-app.html",
    "mobile:sim": "node mobile-pwa/simple-server.js"
  },
  "keywords": [
    "jsx",
    "framework",
    "react",
    "universal",
    "cross-platform",
    "mobile",
    "web",
    "embedded",
    "arduino",
    "esp32",
    "android",
    "ios",
    "desktop",
    "electron",
    "lvgl",
    "gui",
    "embedded-gui",
    "iot",
    "micropython",
    "embedded-systems",
    "gpio",
    "i2c",
    "spi",
    "hardware"
  ],
  "author": "Dolphin Framework Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dolphinjs/dolphin.git"
  },
  "bugs": {
    "url": "https://github.com/dolphinjs/dolphin/issues"
  },
  "homepage": "https://dolphinjs.dev",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-syntax-jsx": "^7.23.3",
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/standalone": "^7.24.0",
    "events": "^3.3.0",
    "jsdom": "^24.0.0",
    "sqlite3": "^5.1.6",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cross-env": "^10.1.0",
    "eslint": "^8.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "gh-pages": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "serve": "^14.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "bin": {
    "dolphin": "./cli/dolphin-cli.js",
    "dolphinjs": "./cli/dolphin-cli.js",
    "create-dolphin-app": "./cli/create-app.js"
  },
  "files": [
    "index.js",
    "dolphin-class.js",
    "core/",
    "systems/",
    "utils/",
    "build/",
    "compiler/",
    "runtime/",
    "embedded/",
    "lvgl/",
    "cli/",
    "examples/",
    "jsx-runtime.js",
    "jsx-dev-runtime.js"
  ]
}